{
  "presets": [
    "es2015",
    "react",
    "stage-0"
  ],
  "env": {
    "development": {
      "plugins": [["react-transform", {
        "transforms": [{
          "transform": [
            "transform-react-jsx-source",
            "transform-class-properties",
            "react-hot-loader/babel"
          ],
          "imports": ["react"],
          "locals": ["module"]
        }]
      }]],
      "presets": [
        "react-hmre"
      ]
    },
    "production": {
      "plugins": []
    }
  }
}
